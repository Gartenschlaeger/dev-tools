{
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.rulers": [120],
	"editor.wordWrap": "on",
	"editor.codeActionsOnSave": {
		"source.organizeImports": true
	},
	"files.exclude": {
		"node_modules": true,
		"dist": true,
		".angular": true
	},
	"typescript.preferences.importModuleSpecifier": "relative"
}
